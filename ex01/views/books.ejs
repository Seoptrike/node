<h1><%=title%></h1>
<div id="books">
    <script id="temp_books" type="x-handlebars-template">
        <div class="row">
            {{#each documents}}
                <div class="col-2 mb-4">
                    <img src={{thumbnail}}>
                    <div class="ellipsis title">{{title}}</div>
                    <div class="price">{{price}}</div>
                </div>
            {{/each}}
        </div>

    </script>
</div>
<script>
    $.ajax({
        type:"get",
        url:"https://dapi.kakao.com/v3/search/book?target=title",
        data:"json",
        headers:{"Authorization":"KakaoAK 6f34b847c930955a9c3b2742745e5a37"},
        data:{"query":"자바",size:12},
        success:function(data){
           // alert("성공");
            console.log(data);
            const temp = Handlebars.compile($("#temp_books").html());
            $("#books").html(temp(data));
        }
    })
</script>
